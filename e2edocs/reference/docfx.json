{
  "build": {
    "content": [
      {
        "files": [
          "**/*.yml", "md/**/*.md", "*.md", "toc.yml"
        ],
        "exclude": [
          "**/obj/**",
          "_themes/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.bmp",
          "**/*.html",
          "_themes/**"
        ],
        "exclude": [
          "**/obj/**",
          "_themes/**"
        ]
      }
    ],
    "globalMetadata": {
      "breadcrumb_path": "/hub/breadcrumb.json",
      "ms.locale": "en-us"
    },
    "template": [
      "docs.html"
    ],
    "dest": "reference"
  }
}
