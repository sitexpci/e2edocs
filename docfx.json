{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md"
        ],
        "exclude": [
          "**/obj/**",
          "_themes/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.bmp",
          "**/*.html",
          "_themes/**",
          "**/breadcrumb.json"
        ],
        "exclude": [
          "**/obj/**",
          "_themes/DocPacker/**"
        ]
      }
    ],
    "dest": "hub",
    "template": "docs.html",
    "globalMetadata": {
      "layout": "Conceptual",
      "breadcrumb_path": "/hub/breadcrumb.json",
      "ms.locale": "en-us"
    }
  }
}
